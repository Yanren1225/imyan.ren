{
  "name": "@yanren/common",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "svelte-package",
    "dev": "svelte-package -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "svelte": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*.svelte": "./src/*.svelte"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
